{
  "name": "@webo-agency/en",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "pnpm upgrade @webo-agency/theme",
    "analyze": "nuxt build --analyze",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "dev-f": "npx wrangler pages dev . --compatibility-date=2024-10-23",
