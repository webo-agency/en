  "packageManager": "pnpm@8.5.1",
  "dependencies": {
    "@cloudflare/pages-plugin-static-forms": "^1.0.3",
    "@webo-agency/theme": "github:webo-agency/www#main",
    "mjml": "^4.15.3",
    "nuxt": "^3.13.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@nuxthq/studio": "^2.1.1",
    "typescript": "^5.6.3"
  }
}
